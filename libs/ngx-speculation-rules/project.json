{
  "name": "ngx-speculation-rules",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ngx-speculation-rules/src",
  "prefix": "speculation-rules",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/ngx-speculation-rules/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ngx-speculation-rules/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ngx-speculation-rules/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ngx-speculation-rules/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/ngx-speculation-rules/**/*.ts",
          "libs/ngx-speculation-rules/**/*.html"
        ]
      }
    },
    "release": {
      "executor": "nx-release:build-update-publish",
      "options": {
        "libName": "ngx-speculation-rules"
      }
    }
  }
}
